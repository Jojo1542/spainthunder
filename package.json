{
  "name": "SpainThunder",
  "version": "0.0.0",
  "description": "",
  "main": "./main.js",
  "scripts": {
    "start": "electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "author": "Jojo1545",
  "license": "ISC",
  "build": {
    "appId": "PruebaJojo",
    "asar": true,
    "dmg": {
      "contents": [
        {
          "x": 110,
          "y": 150
        },
        {
          "x": 240,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ]
    },
    "win": {
      "target": "NSIS",
      "icon": "build/icon.ico"
    }
  },
  "devDependencies": {
    "electron": "^3.0.2",
    "electron-builder": "^20.28.4",
    "electron-installer-squirrel-windows": "^1.3.0",
    "electron-packager": "^12.1.2",
    "electron-winstaller": "^2.7.0"
  },
  "dependencies": {
    "electric-squirrel": "^1.0.2",
    "electron-squirrel-startup": "^1.0.0",
    "update-electron-app": "^1.3.0"
  }
}
